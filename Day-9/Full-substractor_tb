module full_substractor_tb;
reg a,b,c;
wire borrow,difference;
full_substractor dut(.a(a), .b(b), .c(c), .borrow(borrow), .difference(difference));
initial begin
$display("a=%b, b=%b, c=%b, borrow=%b, difference=%b",a,b,c,borrow,difference);
#25; a=0; b=0; c=0;
#25; a=0; b=0; c=1;
#25; a=0; b=1; c=0;
#25; a=0; b=1; c=1;
#25; a=0; b=0; c=0;
#25; a=0; b=0; c=1;
#25; a=0; b=1; c=0;
#25; a=0; b=1; c=1;
#30
$finish;
end
endmodule
